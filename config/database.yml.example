# This is example which you can use for creating your database.yml

default: &default
  adapter: postgresql
  host: 127.0.0.1
  encoding: unicode
  # this is username and password your postgres user
  # if you just setted up Postgres server than you should create it
  user: <user_name>
  password: <password>
  pool: 5
  timeout: 5000


development:
  <<: *default
  database: good_things_development

# Warning: The database defined as "test" will be erased and
# re-generated from your development database when you run "rake".
# Do not set this db to the same as development or production.
test:
  <<: *default
  database: good_things_test
